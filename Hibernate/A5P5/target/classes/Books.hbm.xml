<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.yusuf.spring.pojo.Books" table="newbookstable">
        <id column="id" name="id" type="long">
            <generator class="native" />
        </id>
        <property column="isbn" name="isbn" type="string" />
        <property column="title" name="title" type="string" />
        <property column="author" name="author" type="string" />
        <property column="price" name="price" type="float" />
    </class>
</hibernate-mapping>
